<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-max-width: 400; -fx-max-height: 400; -fx-pref-height: 270;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.indiv.EditWindowController">
   <children>
      <VBox layoutX="6.0" prefHeight="200.0" prefWidth="100.0" style="-fx-min-width: 420; -fx-min-height: 210;">
          <Label fx:id="labelWindow" style="-fx-padding: 10px;" text="label">
              <font>
                  <Font name="System Bold" size="25.0" />
              </font></Label>
         <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-padding: 10px;">
            <children>
               <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-spacing: 10px;">
                  <children>
                     <Label fx:id="time" style="-fx-min-height: 26px; -fx-min-width: 200px;" text="Label">
                        <font>
                           <Font size="17.0" />
                        </font></Label>
                     <Label fx:id="name" style="-fx-min-height: 26px; -fx-min-width: 200px;" text="Label">
                        <font>
                           <Font size="17.0" />
                        </font></Label>
                     <Label fx:id="place" style="-fx-min-height: 26px; -fx-min-width: 200px;" text="Label">
                        <font>
                           <Font size="17.0" />
                        </font></Label>
                     <Label fx:id="notes" style="-fx-min-height: 26px; -fx-min-width: 200px;" text="Label">
                        <font>
                           <Font size="17.0" />
                        </font></Label>
                  </children>
               </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-spacing: 10px;">
                  <children>
                      <TextField fx:id="timeValue" prefHeight="27.0" prefWidth="100.0" style="-fx-min-width: 200px;" />
                      <TextField fx:id="nameValue" style="-fx-min-width: 200px;" />
                      <TextField fx:id="placeValue" style="-fx-min-width: 200px;" />
                      <TextField fx:id="notesValue" style="-fx-min-width: 200px;" />
                  </children>
                </VBox>
            </children>
         </HBox>
      </VBox>
      <Button fx:id="applyButton" layoutX="166.0" layoutY="217.0" mnemonicParsing="false" prefHeight="275.0" style="-fx-pref-height: 30; -fx-pref-width: 100;" text="Применить" onAction="#applyButtonClicked" />
   </children>
</AnchorPane>
